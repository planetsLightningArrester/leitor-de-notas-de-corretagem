  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "Parse",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "program": "${workspaceFolder}/dist/parse.js",
      "args": [
        "C:\\Users\\franc\\repos\\notas\\src\\tests\\notes"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "name": "Jest",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "--runInBand",
        "rico.test.ts"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229
    }
  ]
}